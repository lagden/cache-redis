{
  "name": "@tadashi/cache-redis",
  "version": "4.1.2",
  "description": "Using redis as cache",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava -t"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lagden/cache-redis.git"
  },
  "keywords": [
    "cache",
    "redis",
    "lib"
  ],
  "engines": {
    "node": ">=8.9.1"
  },
  "author": "Thiago Lagden",
  "license": "MIT",
  "homepage": "https://github.com/lagden/cache-redis#readme",
  "bugs": {
    "url": "https://github.com/lagden/cache-redis/issues"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "unicorn/prevent-abbreviations": 0
    }
  },
  "dependencies": {
    "@tadashi/jsonb": "3.0.0",
    "ioredis": "4.16.0"
  },
  "devDependencies": {
    "ava": "3.5.0",
    "xo": "0.28.0"
  }
}
