    "@tadashi/connect-redis": ">= 3 < 4",
    "ioredis": ">= 4.19.4 < 6"
  },
  "devDependencies": {
    "@tadashi/connect-redis": "3.0.0",
    "@types/node": "22.5.4",
    "c8": "10.1.2",
    "ioredis": "5.4.1",
    "p-event": "6.0.1",
    "typescript": "5.5.4"
  }
