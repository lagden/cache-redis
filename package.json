{
  "name": "@tadashi/cache-redis",
  "version": "5.0.1",
  "description": "Using redis as cache",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "pretest": "xo",
    "test": "ava -t",
    "precoverage": "npm run pretest",
    "coverage": "nyc --reporter=text-summary --reporter=lcov ava"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lagden/cache-redis.git"
  },
  "keywords": [
    "cache",
    "redis",
    "lib"
  ],
  "engines": {
    "node": ">=14"
  },
  "author": "Thiago Lagden",
  "license": "MIT",
  "homepage": "https://github.com/lagden/cache-redis#readme",
  "bugs": {
    "url": "https://github.com/lagden/cache-redis/issues"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "unicorn/prevent-abbreviations": 0
    }
  },
  "dependencies": {
    "@tadashi/connect-redis": "1.0.1",
    "@tadashi/jsonb": "5.0.1"
  },
  "peerDependencies": {
    "ioredis": ">= 4.19.4 < 5"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "ioredis": "4.24.5",
    "nyc": "15.1.0",
    "xo": "0.38.2"
  }
}
